{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "alexusmai/laravel-file-manager": "^2.4",
        "anlutro/l4-settings": "^0.8.3",
        "axelrindle/larecipe-emojify": "^1.0",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "binarytorch/larecipe": "^2.1",
        "chumper/zipper": "^1.0",
        "darryldecode/cart": "^4.0",
        "eclipxe/cfdiutils": "^2.10",
        "eduardokum/laravel-mail-auto-embed": "^1.0",
        "fideloper/proxy": "^4.0",
        "gerardojbaez/money": "0.*",
        "guzzlehttp/guzzle": "^6.3",
        "homedesignshops/laravel-zendesk": "^1.0",
        "intervention/image": "^2.4",
        "jenssegers/date": "^3.5",
        "kwn/number-to-words": "^1.6",
        "kyslik/column-sortable": "5.8.*",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "openpay/sdk": "dev-master",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "spatie/laravel-newsletter": "^4.4",
        "spatie/laravel-permission": "^2.37",
        "yajra/laravel-datatables-oracle": "^9.7"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
